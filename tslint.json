{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-config-prettier"
    ],
    "jsRules": {},
    "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*."]
    },
    "rules": {
        "no-console": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-imports": true,
        "interface-name": false,
        "strict-null-checks": false
    },
    "rulesDirectory": []
}